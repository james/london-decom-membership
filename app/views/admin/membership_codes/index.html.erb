<h2><%= t('.generate_codes') %></h2>
<%= bootstrap_form_tag url: admin_membership_codes_path do |f| %>
  <%= f.number_field :number, label: t('.codes_to_generate') %>
  <%= f.submit t('.create') %>
<% end %>

<h2><%= t('.all_codes') %></h2>

<p><%= t('.code_count', assigned: MembershipCode.taken.count, remaining: MembershipCode.available.count) %></p>

<p>
  <%= t('.import_to_eventbrite') %>
  <%= link_to t('.download_csv'), admin_membership_codes_path(format: :csv) %>
</p>

<p><%= t('.readme_html') %></p>
